{
  "name": "kseven/filament-multiblog",
  "description": "Firefly/Filament-Blog plugin extension with site_id support (multi-ban by site).",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "K'Seven",
      "email": "anjsuporte@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "firefly/filament-blog": "^2.0",
    "filament/filament": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Kseven\\FilamentMultiBlog\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kseven\\FilamentMultiBlog\\Providers\\MultiBlogServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
